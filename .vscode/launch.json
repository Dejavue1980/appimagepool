    "version": "0.2.0",
    "configurations": [
        {
            "name": "Flutter: Attach to Process",
            "type": "dart",
            "request": "attach",
            "osx": {
                "name": "Launch in OSX",
                "request": "attach",
                "type": "dart",
                "serviceInfoFile": "${env:TMPDIR}vmservice.appimagepool",
            },
            "windows": {
                "name": "Launch in Windows",
                "request": "attach",
                "type": "dart",
                "serviceInfoFile": "${env:TEMP}/vmservice.appimagepool"
            },
            "linux": {
                "name": "Launch in Linux",
                "request": "attach",
                "type": "dart",
                "serviceInfoFile": "${env:XDG_RUNTIME_DIR}/vmservice.appimagepool"
            }
        },
    ]
}